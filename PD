-- Valid keys
local validKeys = {
    ["G1T7XQ4Mkey9A2Z8K5R0N6"] = true,
    ["fgftop1"] = true,
}

-- Copy Discord link
G2L["5"].MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/ttQMyKvKt6")
    G2L["5"].Text = "Copied!"
    task.delay(1.5, function()
        G2L["5"].Text = "Copy link key"
    end)
end)

-- Load key and launch loader
G2L["6"].MouseButton1Click:Connect(function()
    local input = G2L["3"].Text
    if validKeys[input] then
        G2L["6"].Text = "Access Granted!"
        getgenv().Key = "Lirp**@!(#%*(Fixed*(!@%*(%!@*(%!*@(Yey*!@%@!OOOW59@(!%!@%%!@"
        task.wait(1)
        G2L["1"]:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/LirpOfficial/Lirp/refs/heads/main/Loader", true))()
    else
        G2L["6"].Text = "Invalid Key"
    end
    task.delay(2, function()
        if G2L["1"] and G2L["6"] then
            G2L["6"].Text = "LoadKey"
        end
    end)
end)
