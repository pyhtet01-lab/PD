-- üìã –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ Discord-—Å—Å—ã–ª–∫–∏
G2L["8"].MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/ttQMyKvKt6")
    game.StarterGui:SetCore("SendNotification", {
        Title = "Copied",
        Text = "Discord link copied to clipboard.",
        Duration = 4,
    })
end)

-- ‚úÖ –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞ –∏ –∑–∞–ø—É—Å–∫ –∑–∞–≥—Ä—É–∑—á–∏–∫–∞
G2L["a"].MouseButton1Click:Connect(function()
    if G2L["6"].Text == "G1T7XQ4Mkey9A2Z8K5R0N6" then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Access Granted",
            Text = "Key is valid.",
            Duration = 5,
        })
        G2L["1"]:Destroy()
        getgenv().Key = "Lirp**@!(#%*(Fixed*(!@%*(%!@*(%!*@(Yey*!@%@!OOOW59@(!%!@%%!@"
        loadstring(game:HttpGet("https://raw.githubusercontent.com/LirpOfficial/Lirp/refs/heads/main/Loader", true))()
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Access Denied",
            Text = "Invalid key.",
            Duration = 5,
        })
    end
end)

return G2L["1"], require;

